{
  "authenticationMethod": "route",
  "routes": [
    {
      "source": "^/$",
      "target": "/",
      "destination": "flask-backend",
      "authenticationType": "xsuaa"
    },
    {
      "source": "^/debug-(.*)$",
      "target": "/debug-$1", 
      "destination": "flask-backend",
      "authenticationType": "none"
    },
    {
      "source": "^/(.*)$",
      "target": "/$1",
      "destination": "flask-backend", 
      "authenticationType": "xsuaa"
    }
  ]
}